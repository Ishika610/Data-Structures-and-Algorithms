#include <bits/stdc++.h>
using namespace std;

int main()
{
    int A[4][4] = {{1,4,7,11},{2,5,8,12},{3,6,9,16},{10,13,14,17}};
    int k;
    cin>>k;
    for(int i=0;i<4;i++)
    {
        for(int j=0;j<4;j++)
        {
            if(A[i][j]==k)
            {
                cout<<"found";
            }
            else
            {
                cout<<"not found";
            }
            
        }
    }
    return 0;
}
