#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    char ar[n+1];
    cin>>ar;
    int i=0;
    int count=1;
    for(int i=0;i<n;i++)
    {
        if(ar[i]!= ar[n-1-i])
        {
            count = 0 ;
        }
    }
    if(count == 1)
    {
        cout<<"word is pallindrom";
    }
    return 0;
}
